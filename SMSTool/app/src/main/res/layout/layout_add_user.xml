<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ll_store_info"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:visibility="visible">
    <com.wondershare.dfnexpert.widgets.IconEditTextView
        android:id="@+id/iet_name_store_apply"
        style="@style/iet_height_wrap_content_store"
        app:maxLength="128"
        app:isHideIcon="true"
        app:edtIcon="@drawable/ic_store_blue_24dp"
        app:edtHint="@string/name_store_apply"/>


    <com.wondershare.dfnexpert.widgets.IconEditTextView
        android:id="@+id/iet_address_store_apply"
        style="@style/iet_height_wrap_content_store"
        app:maxLength="128"
        app:isHideIcon="true"
        app:edtIcon="@drawable/ic_store_blue_24dp"
        app:edtHint="@string/address_store_apply"/>


</LinearLayout>